{
  "name": "stalker-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cicd": "ng test --watch=false --browsers=ChromeHeadless",
    "format:write": "prettier --write \"src/**/*.{ts,html,scss}\"",
    "lint:write": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-staged:run": "npm run lint:write && npm run format:write",
    "lint-staged": "lint-staged",
    "extract-i18n": "ng extract-i18n --output-path src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular-material-components/color-picker": "^16.0.1",
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@types/angular-material": "^1.1.73",
    "ip-address": "^8.1.0",
    "jwt-decode": "^3.1.2",
    "monaco-editor": "^0.34.1",
    "ngx-file-drop": "^13.0.0",
    "ngx-socket-io": "~4.4.0",
    "ngx-toastr": "~16.2.0",
    "prettier": "^2.7.1",
    "rxjs": "~7.5.0",
    "ts-md5": "^1.3.1",
    "tslib": "^2.3.0",
    "yaml": "^2.1.3",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.11",
    "@angular/cli": "^16.2.11",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/localize": "^16.2.12",
    "@types/jasmine": "~3.10.0",
    "@types/jsbn": "^1.2.30",
    "@types/md5": "^2.3.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typemoq": "^2.1.0",
    "typescript": "~4.9.0"
  },
  "lint-staged": {
    "*.ts": "npm run lint-staged:run"
  }
